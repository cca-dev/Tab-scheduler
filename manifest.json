{
  "manifest_version": 3,
  "name": "CCA Kiosk Tab Scheduler",
  "version": "1.2",
  "permissions": [
    "tabs",
    "alarms",
    "storage"
  ],
  "host_permissions": [
    "<all_urls>",
    "file:///*",
    "https://ccc.local:44300/*",
    "http://ccc.local:44300/*"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup.html"
  }
}